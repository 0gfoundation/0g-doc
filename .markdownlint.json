{
  "default": true,
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2
  },
  "MD013": {
    "line_length": 100,
    "code_blocks": false,
    "tables": false
  },
  "MD024": {
    "allow_different_nesting": true
  },
  "MD025": {
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "MD033": {
    "allowed_elements": [
      "a",
      "abbr",
      "acronym",
      "b",
      "br",
      "code",
      "del",
      "em",
      "i",
      "img",
      "ins",
      "kbd",
      "mark",
      "s",
      "strong",
      "sub",
      "sup",
      "u",
      "details",
      "summary",
      "div",
      "span",
      "table",
      "thead",
      "tbody",
      "tr",
      "th",
      "td"
    ]
  },
  "MD036": false,
  "MD041": false,
  "MD046": {
    "style": "fenced"
  }
}